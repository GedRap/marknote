<div class="row">
	<div class="col-md-12">
		<h3>Browse Notes</h3>
	</div>
	<div class="col-md-12">
		<% @notes.each do |note| %>
			<div class="row">
				<div class="col-md-12">
					<p><a href="/notes/<%= note.id %>"><%= note.title %></a></p>
					<p class="timestamp"><%= note.created_at %>, Views: <%= note.views %></p>
				</div>
			</div>
		<% end %>
	</div>
</div>
